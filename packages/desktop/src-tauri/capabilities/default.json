{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:allow-show",
    "core:window:allow-start-dragging",
    "opener:default",
    "http:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [{ "url": "http://127.0.0.1:6969/*" }]
    }
  ]
}
